author = {Aroom, Kevin R. and Harting, Matthew T. and Cox, Charles S. and Radharkrishnan, Ravi S. and Smith, Carter and Gill, Brijesh S.},
doi = {10.1016/j.jss.2008.04.019},
eprint = {NIHMS150003},
file = {:C$\backslash$:/Users/mbana/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Aroom et al. - 2009 - Bioimpedance Analysis A Guide to Simple Design and Implementation.pdf:pdf},
isbn = {2122633255},
