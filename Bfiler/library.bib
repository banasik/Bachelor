isbn = {0179-051X (Print)$\backslash$n0179-051X (Linking)},
issn = {0179051X},
journal = {Dysphagia},
keywords = {Adults,Deglutition,Deglutition disorders,Dysphagia,Reliability,Videofluoroscopy},
number = {2},
pages = {110--118},
pmid = {11305220},
title = {{Inter- and intrajudge reliability for videofluoroscopic swallowing evaluation measures}},
volume = {16},
